using TMINHLE_PURCHASEORDER_ITEMCATEGORIES_TEXTS as TMINHLE_PURCHASEORDER_ITEMCATEGORIES_TEXTS;
entity TMINHLE_PURCHASEORDER_ITEMCATEGORIES {
  NAME : String(255);
  DESCR : String(1000);
  key CODE : String(1) not null;
  TEXTS : association[*] to TMINHLE_PURCHASEORDER_ITEMCATEGORIES_TEXTS on TEXTS.CODE = CODE;
  LOCALIZED : association to TMINHLE_PURCHASEORDER_ITEMCATEGORIES_TEXTS on LOCALIZED.CODE = CODE and LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
};
