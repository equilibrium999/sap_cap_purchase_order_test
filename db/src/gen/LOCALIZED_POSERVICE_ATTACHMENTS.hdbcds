using LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS as LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS;
using LOCALIZED_POSERVICE_PURCHASEORDERS as LOCALIZED_POSERVICE_PURCHASEORDERS;
using LOCALIZED_POSERVICE_PURCHASEORDERITEMS as LOCALIZED_POSERVICE_PURCHASEORDERITEMS;
view LOCALIZED_POSERVICE_ATTACHMENTS as select from LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS as ATTACHMENTS_0 mixin {
  ___PARENTPURCHASEORDER : association to LOCALIZED_POSERVICE_PURCHASEORDERS on ___PARENTPURCHASEORDER.ID = $projection.PARENTPURCHASEORDER_ID;
  ___PARENTORDERITEM : association to LOCALIZED_POSERVICE_PURCHASEORDERITEMS on ___PARENTORDERITEM.ID = $projection.PARENTORDERITEM_ID;
} into {
  key ATTACHMENTS_0.ID as ID,
  ATTACHMENTS_0.PURCHASEORDER,
  ATTACHMENTS_0.PURCHASEORDERVERSION,
  ATTACHMENTS_0.PURCHASEORDERITEM,
  ATTACHMENTS_0.FILEID,
  ATTACHMENTS_0.FILENAME,
  ATTACHMENTS_0.FILESIZE,
  ATTACHMENTS_0.MIMETYPE,
  ___PARENTPURCHASEORDER as PARENTPURCHASEORDER,
  ATTACHMENTS_0.PARENTPURCHASEORDER_ID,
  ___PARENTORDERITEM as PARENTORDERITEM,
  ATTACHMENTS_0.PARENTORDERITEM_ID
};
