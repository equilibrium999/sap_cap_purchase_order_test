  L.CREATEDAT,
  L.CREATEDBY,
  L.MODIFIEDAT,
  L.MODIFIEDBY,
  L.PURCHASEORDER,
  L.PURCHASEORDERVERSION,
  L.PURCHASEORDERITEM,
  L.SCHEDULELINE,
  L.DATEOFDELIVERYCATEGORY,
  L.DATEOFDELIVERY,
  L.SCHEDULELINEQUANTITY,
  L.QUANTITYUNIT,
  L.CONFIRMEDQUANTITY,
  L.CONFIRMEDDATEOFDELIVERY,
  ___PARENTORDERITEM as PARENTORDERITEM,
  L.PARENTORDERITEM_ID
};
