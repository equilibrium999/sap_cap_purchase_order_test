using SAP_COMMON_LANGUAGES_TEXTS as SAP_COMMON_LANGUAGES_TEXTS;
entity SAP_COMMON_LANGUAGES {
  NAME : String(255);
  DESCR : String(1000);
  key CODE : String(14);
  TEXTS : association[*] to SAP_COMMON_LANGUAGES_TEXTS on TEXTS.CODE = CODE;
  LOCALIZED : association to SAP_COMMON_LANGUAGES_TEXTS on LOCALIZED.CODE = CODE and LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
};
