using TMINHLE_PURCHASEORDER_PURCHASEORDERITEMS as TMINHLE_PURCHASEORDER_PURCHASEORDERITEMS;
using LOCALIZED_TMINHLE_PURCHASEORDER_ITEMCATEGORIES as LOCALIZED_TMINHLE_PURCHASEORDER_ITEMCATEGORIES;
using LOCALIZED_TMINHLE_PURCHASEORDER_MATERIALS as LOCALIZED_TMINHLE_PURCHASEORDER_MATERIALS;
using LOCALIZED_TMINHLE_PURCHASEORDER_MATERIALGROUPS as LOCALIZED_TMINHLE_PURCHASEORDER_MATERIALGROUPS;
using LOCALIZED_TMINHLE_PURCHASEORDER_PLANTS as LOCALIZED_TMINHLE_PURCHASEORDER_PLANTS;
using LOCALIZED_SAP_COMMON_CURRENCIES as LOCALIZED_SAP_COMMON_CURRENCIES;
using TMINHLE_PURCHASEORDER_ADDRESSES as TMINHLE_PURCHASEORDER_ADDRESSES;
using LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERS as LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERS;
using LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS as LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS;
using LOCALIZED_TMINHLE_PURCHASEORDER_SCHEDULELINES as LOCALIZED_TMINHLE_PURCHASEORDER_SCHEDULELINES;
view LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERITEMS as select from TMINHLE_PURCHASEORDER_PURCHASEORDERITEMS as L mixin {
  ___PURCHASEORDERITEMCAT : association to LOCALIZED_TMINHLE_PURCHASEORDER_ITEMCATEGORIES on ___PURCHASEORDERITEMCAT.CODE = $projection.PURCHASEORDERITEMCAT_CODE;
  ___MATERIAL : association to LOCALIZED_TMINHLE_PURCHASEORDER_MATERIALS on ___MATERIAL.CODE = $projection.MATERIAL_CODE;
  ___MATERIALGROUP : association to LOCALIZED_TMINHLE_PURCHASEORDER_MATERIALGROUPS on ___MATERIALGROUP.CODE = $projection.MATERIALGROUP_CODE;
  ___PLANT : association to LOCALIZED_TMINHLE_PURCHASEORDER_PLANTS on ___PLANT.CODE = $projection.PLANT_CODE;
  ___SUPPLIERMATERIAL : association to LOCALIZED_TMINHLE_PURCHASEORDER_MATERIALS on ___SUPPLIERMATERIAL.CODE = $projection.SUPPLIERMATERIAL_CODE;
  ___DOCUMENTCURRENCY : association to LOCALIZED_SAP_COMMON_CURRENCIES on ___DOCUMENTCURRENCY.CODE = $projection.DOCUMENTCURRENCY_CODE;
  ___DELIVERYADDRESS : association to TMINHLE_PURCHASEORDER_ADDRESSES on ___DELIVERYADDRESS.CODE = $projection.DELIVERYADDRESS_CODE;
  ___PARENTPURCHASEORDER : association to LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERS on ___PARENTPURCHASEORDER.ID = $projection.PARENTPURCHASEORDER_ID;
  ___ATTACHMENTS : association[*] to LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS on ___ATTACHMENTS.PARENTORDERITEM_ID = $projection.ID;
  ___SCHEDULELINES : association[*] to LOCALIZED_TMINHLE_PURCHASEORDER_SCHEDULELINES on ___SCHEDULELINES.PARENTORDERITEM_ID = $projection.ID;
} into {
  key L.ID as ID,
  L.CREATEDAT,
  L.CREATEDBY,
  L.MODIFIEDAT,
  L.MODIFIEDBY,
  L.PURCHASEORDER,
  L.PURCHASEORDERVERSION,
  L.PURCHASEORDERITEM,
  ___PURCHASEORDERITEMCAT as PURCHASEORDERITEMCAT,
  L.PURCHASEORDERITEMCAT_CODE,
  ___MATERIAL as MATERIAL,
  L.MATERIAL_CODE,
  ___MATERIALGROUP as MATERIALGROUP,
  L.MATERIALGROUP_CODE,
  ___PLANT as PLANT,
  L.PLANT_CODE,
  ___SUPPLIERMATERIAL as SUPPLIERMATERIAL,
  L.SUPPLIERMATERIAL_CODE,
  L.ORDERQUANTITY,
  L.QUANTITYUNIT,
  L.ORDERPRICE,
  ___DOCUMENTCURRENCY as DOCUMENTCURRENCY,
  L.DOCUMENTCURRENCY_CODE,
  L.PRICEUNIT,
  L.NETVALUE,
  L.GROSSVALUE,
  L.DATEOFDELIVERY,
  L.SCHEDULELINEINC,
  L.CONFIRMATIONORDER,
  L.CONFIRMATIONINC,
  L.CONFIRMEDQUANTITY,
  L.CONFIRMEDPRICE,
  L.CONFIRMEDDATEOFDELIVERY,
  L.GOODRECEIPT,
  L.UNLOADINGPOINT,
  L.INCOTERMVERION,
  L.INCOTERMCLASSIFICATION,
  L.INCOTERMLOCATION1,
  L.INCOTERMLOCATION2,
  L.LONGPLAINTEXT,
  ___DELIVERYADDRESS as DELIVERYADDRESS,
  L.DELIVERYADDRESS_CODE,
  ___PARENTPURCHASEORDER as PARENTPURCHASEORDER,
  L.PARENTPURCHASEORDER_ID,
  ___ATTACHMENTS as ATTACHMENTS,
  ___SCHEDULELINES as SCHEDULELINES
};
