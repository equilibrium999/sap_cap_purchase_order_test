using TMINHLE_PURCHASEORDER_PURCHASEORDERS as TMINHLE_PURCHASEORDER_PURCHASEORDERS;
using LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERSTATUSES as LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERSTATUSES;
using LOCALIZED_SAP_COMMON_LANGUAGES as LOCALIZED_SAP_COMMON_LANGUAGES;
using TMINHLE_PURCHASEORDER_ADDRESSES as TMINHLE_PURCHASEORDER_ADDRESSES;
using LOCALIZED_SAP_COMMON_CURRENCIES as LOCALIZED_SAP_COMMON_CURRENCIES;
using LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGORGANIZATIONS as LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGORGANIZATIONS;
using LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGGROUPS as LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGGROUPS;
using LOCALIZED_TMINHLE_PURCHASEORDER_COMPANYCODES as LOCALIZED_TMINHLE_PURCHASEORDER_COMPANYCODES;
using LOCALIZED_TMINHLE_PURCHASEORDER_TERMPAYMENTS as LOCALIZED_TMINHLE_PURCHASEORDER_TERMPAYMENTS;
using TMINHLE_PURCHASEORDER_CONTACTPERSONS as TMINHLE_PURCHASEORDER_CONTACTPERSONS;
using LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS as LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS;
using LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERITEMS as LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERITEMS;
view LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERS as select from TMINHLE_PURCHASEORDER_PURCHASEORDERS as L mixin {
  ___PURCHASEORDERSTATUS : association to LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERSTATUSES on ___PURCHASEORDERSTATUS.CODE = $projection.PURCHASEORDERSTATUS_CODE;
  ___LANGUAGE : association to LOCALIZED_SAP_COMMON_LANGUAGES on ___LANGUAGE.CODE = $projection.LANGUAGE_CODE;
  ___SUPPLIERADDRESS : association to TMINHLE_PURCHASEORDER_ADDRESSES on ___SUPPLIERADDRESS.CODE = $projection.SUPPLIERADDRESS_CODE;
  ___DOCUMENTCURRENCY : association to LOCALIZED_SAP_COMMON_CURRENCIES on ___DOCUMENTCURRENCY.CODE = $projection.DOCUMENTCURRENCY_CODE;
  ___PURCHASINGORGANIZATION : association to LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGORGANIZATIONS on ___PURCHASINGORGANIZATION.CODE = $projection.PURCHASINGORGANIZATION_CODE;
  ___PURCHASINGGROUP : association to LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGGROUPS on ___PURCHASINGGROUP.CODE = $projection.PURCHASINGGROUP_CODE;
  ___COMPANYCODE : association to LOCALIZED_TMINHLE_PURCHASEORDER_COMPANYCODES on ___COMPANYCODE.CODE = $projection.COMPANYCODE_CODE;
  ___TERMOFPAYMENT : association to LOCALIZED_TMINHLE_PURCHASEORDER_TERMPAYMENTS on ___TERMOFPAYMENT.CODE = $projection.TERMOFPAYMENT_CODE;
  ___CONTACTPERSON : association to TMINHLE_PURCHASEORDER_CONTACTPERSONS on ___CONTACTPERSON.CODE = $projection.CONTACTPERSON_CODE;
  ___DELIVERYADDRESS : association to TMINHLE_PURCHASEORDER_ADDRESSES on ___DELIVERYADDRESS.CODE = $projection.DELIVERYADDRESS_CODE;
  ___ATTACHMENTS : association[*] to LOCALIZED_TMINHLE_PURCHASEORDER_ATTACHMENTS on ___ATTACHMENTS.PARENTPURCHASEORDER_ID = $projection.ID;
  ___ITEMS : association[*] to LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASEORDERITEMS on ___ITEMS.PARENTPURCHASEORDER_ID = $projection.ID;
} into {
  key L.ID as ID,
  L.CREATEDAT,
  L.CREATEDBY,
  L.MODIFIEDAT,
  L.MODIFIEDBY,
  L.PURCHASEORDER,
  L.PURCHASEORDERVERSION,
  L.PURCHASEORDERDESC,
  ___PURCHASEORDERSTATUS as PURCHASEORDERSTATUS,
  L.PURCHASEORDERSTATUS_CODE,
  L.PURCHASEORDERDATE,
  L.TITLE,
  ___LANGUAGE as LANGUAGE,
  L.LANGUAGE_CODE,
  L.SUPPLIER,
  L.SUPPLIERNAME,
  ___SUPPLIERADDRESS as SUPPLIERADDRESS,
  L.SUPPLIERADDRESS_CODE,
  L.TOTALAMOUNT,
  ___DOCUMENTCURRENCY as DOCUMENTCURRENCY,
  L.DOCUMENTCURRENCY_CODE,
  L.EXCHANGERATE,
  ___PURCHASINGORGANIZATION as PURCHASINGORGANIZATION,
  L.PURCHASINGORGANIZATION_CODE,
  ___PURCHASINGGROUP as PURCHASINGGROUP,
  L.PURCHASINGGROUP_CODE,
  ___COMPANYCODE as COMPANYCODE,
  L.COMPANYCODE_CODE,
  L.ORDERCONFIRMATION,
  L.INCOTERMVERION,
  L.INCOTERMCLASSIFICATION,
  L.INCOTERMLOCATION1,
  L.INCOTERMLOCATION2,
  ___TERMOFPAYMENT as TERMOFPAYMENT,
  L.TERMOFPAYMENT_CODE,
  L.NETPAYMENTTERM,
  L.CASHDISCOUNT1DAYS,
  L.CASHDISCOUNT1PERCENT,
  L.CASHDISCOUNT2DAYS,
  L.CASHDISCOUNT2PERCENT,
  L.DATEOFDELIVERY,
  L.STARTPERIOD,
  L.ENDPERIOD,
  L.PLAINLONGTEXT,
  ___CONTACTPERSON as CONTACTPERSON,
  L.CONTACTPERSON_CODE,
  ___DELIVERYADDRESS as DELIVERYADDRESS,
  L.DELIVERYADDRESS_CODE,
  ___ATTACHMENTS as ATTACHMENTS,
  ___ITEMS as ITEMS
};
