using TMINHLE_PURCHASEORDER_PURCHASEORDERSTATUSES_TEXTS as TMINHLE_PURCHASEORDER_PURCHASEORDERSTATUSES_TEXTS;
entity TMINHLE_PURCHASEORDER_PURCHASEORDERSTATUSES {
  CREATEDAT : UTCTimestamp;
  CREATEDBY : String(255);
  MODIFIEDAT : UTCTimestamp;
  MODIFIEDBY : String(255);
  NAME : String(255);
  DESCR : String(1000);
  key CODE : String(2) not null;
  TEXTS : association[*] to TMINHLE_PURCHASEORDER_PURCHASEORDERSTATUSES_TEXTS on TEXTS.CODE = CODE;
  LOCALIZED : association to TMINHLE_PURCHASEORDER_PURCHASEORDERSTATUSES_TEXTS on LOCALIZED.CODE = CODE and LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
}
technical configuration {
  unique index NAME on (NAME);
};
