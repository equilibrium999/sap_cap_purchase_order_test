using TMINHLE_PURCHASEORDER_TERMPAYMENTS as TMINHLE_PURCHASEORDER_TERMPAYMENTS;
using TMINHLE_PURCHASEORDER_TERMPAYMENTS_TEXTS as TMINHLE_PURCHASEORDER_TERMPAYMENTS_TEXTS;
view LOCALIZED_TMINHLE_PURCHASEORDER_TERMPAYMENTS as select from (TMINHLE_PURCHASEORDER_TERMPAYMENTS as L_0 left join TMINHLE_PURCHASEORDER_TERMPAYMENTS_TEXTS as LOCALIZED_1 on LOCALIZED_1.CODE = L_0.CODE and LOCALIZED_1.LOCALE = SESSION_CONTEXT('LOCALE')) mixin {
  ___TEXTS : association[*] to TMINHLE_PURCHASEORDER_TERMPAYMENTS_TEXTS on ___TEXTS.CODE = $projection.CODE;
  ___LOCALIZED : association to TMINHLE_PURCHASEORDER_TERMPAYMENTS_TEXTS on ___LOCALIZED.CODE = $projection.CODE and ___LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
} into {
  coalesce(LOCALIZED_1.NAME, L_0.NAME) as NAME,
  coalesce(LOCALIZED_1.DESCR, L_0.DESCR) as DESCR,
  key L_0.CODE as CODE,
  ___TEXTS as TEXTS,
  ___LOCALIZED as LOCALIZED
};
