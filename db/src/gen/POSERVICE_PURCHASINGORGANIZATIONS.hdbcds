using TMINHLE_PURCHASEORDER_PURCHASINGORGANIZATIONS as TMINHLE_PURCHASEORDER_PURCHASINGORGANIZATIONS;
using POSERVICE_PURCHASINGORGANIZATIONS_TEXTS as POSERVICE_PURCHASINGORGANIZATIONS_TEXTS;
view POSERVICE_PURCHASINGORGANIZATIONS as select from TMINHLE_PURCHASEORDER_PURCHASINGORGANIZATIONS as PURCHASINGORGANIZATIONS_0 mixin {
  ___TEXTS : association[*] to POSERVICE_PURCHASINGORGANIZATIONS_TEXTS on ___TEXTS.CODE = $projection.CODE;
  ___LOCALIZED : association to POSERVICE_PURCHASINGORGANIZATIONS_TEXTS on ___LOCALIZED.CODE = $projection.CODE and ___LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
} into {
  PURCHASINGORGANIZATIONS_0.NAME,
  PURCHASINGORGANIZATIONS_0.DESCR,
  key PURCHASINGORGANIZATIONS_0.CODE as CODE,
  ___TEXTS as TEXTS,
  ___LOCALIZED as LOCALIZED
};
