using LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGGROUPS as LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGGROUPS;
using POSERVICE_PURCHASINGGROUPS_TEXTS as POSERVICE_PURCHASINGGROUPS_TEXTS;
view LOCALIZED_POSERVICE_PURCHASINGGROUPS as select from LOCALIZED_TMINHLE_PURCHASEORDER_PURCHASINGGROUPS as PURCHASINGGROUPS_0 mixin {
  ___TEXTS : association[*] to POSERVICE_PURCHASINGGROUPS_TEXTS on ___TEXTS.CODE = $projection.CODE;
  ___LOCALIZED : association to POSERVICE_PURCHASINGGROUPS_TEXTS on ___LOCALIZED.CODE = $projection.CODE and ___LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
} into {
  PURCHASINGGROUPS_0.NAME,
  PURCHASINGGROUPS_0.DESCR,
  key PURCHASINGGROUPS_0.CODE as CODE,
  ___TEXTS as TEXTS,
  ___LOCALIZED as LOCALIZED
};
